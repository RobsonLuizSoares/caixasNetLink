
<%- include('headerAdmin') %>
<div class=" adm container">
    <h1 class="mt-4 text-center"> Consulta dos Caixas</h1>
    <hr />
    <div class="container text-center"> 
        <h3> Consultar caixas por Loja</h3>
            <br><% stores.name %>
            <div class="btn-group mr-2 " role="group" aria-label="First group">
                    <a href="/admin/caixasloja/Avenida"><button type="button" class="btn btn-primary">Avenida</button></a>
                    &nbsp;<a href="/admin/caixasloja/Unespar"><button type="button" class="btn btn-primary">Unespar</button></a>
                    &nbsp;<a href="/admin/caixasloja/São%20Cristóvão"><button type="button" class="btn btn-primary">São Cristóvão</button></a>
                    &nbsp;<a href="/admin/caixasloja/Cruz%20Machado"> <button type="button" class="btn btn-primary">Cruz Machado</button></a>
            </div>
    </div>
    <hr />
    
 <% values.forEach(info => { %>
    <div class="row">
        <div class=" col-md-10 m-0 mx-auto">
            <div class="card my-2">
                <div class="card-body my-3">
                    <table class=" col-md-10 mx-auto  table table-sm table-bordered table-info table-hover ">
                        <tbody>
                            <tr class="row table-success px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Loja:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.store.name %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Data:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= moment(info.date).format("DD [de] MMMM [de] YYYY") %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Funcionária:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.name %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Cópias Próprias:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.copyP %>
                                </td>  
                            </tr> 
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Cópias Locadas:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.copyL %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Impressões Próprias:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.printP %>
                                </td>  
                            </tr> 
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Impressões Locadas:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.printL %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Laser Color:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.laserC %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Jato Color:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.jetC %>
                                </td>  
                            </tr> 
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Plotter:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.plotter %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Produtos:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.products %>
                                </td>  
                            </tr> 
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Internet:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.internet %>
                                </td>  
                            </tr>
                            <tr class="row px-0 mx-0">
                                <th class=" col-md-3 " >
                                    <h6 class="text-center my-auto">Observações:</h6>
                                </th>
                                <td class=" pl-5 my-auto  col-md-9">
                                    <%= info.obs %>
                                </td>  
                            </tr>
                        </tbody>
                    </table>
                </div> 
            </div>
        </div>
    </div>
    <% }) %>  
</div>
<%- include('footer') %>